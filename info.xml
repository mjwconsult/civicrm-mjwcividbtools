<?xml version="1.0"?>
<extension key="mjwcividbtools" type="module">
  <file>mjwcividbtools</file>
  <name>MJW CiviCRM DB Tools</name>
  <description>This extension allows you to perform various destructive operations on the database (useful during deployment, do NOT use in production!)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Matthew Wire (MJW Consulting)</author>
    <email>mjw@mjwconsult.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/mattwire/civicrm-mjwcividbtools</url>
    <url desc="Support">https://mjw.pt/support/cividbtools</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-09-11</releaseDate>
  <version>0.7</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Mjwcividbtools</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
